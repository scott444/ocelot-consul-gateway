{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Seq" ],
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341",
          "apiKey": "<API key here>"
        }
      }
    ]
  },
  "ConsulConfig": {
    "Address": "http://192.168.243.23:8500",    
    "ServiceName": "Minimal-API",
    "ServiceID": "minimal-api"
  },
  "AllowedHosts": "*"
}
