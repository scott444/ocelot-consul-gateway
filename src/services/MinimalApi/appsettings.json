{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Seq" ],
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341",
          "apiKey": "<API key here>"
        }
      }
    ]
  },
  "ConsulConfig": {
    "ConsulHost": "localhost",
    "ConsulPort": 8500,
    "ServiceName": "MinimalApi",
    "ServiceId": "minimal-api-1"
  },
  "AllowedHosts": "*"
}
